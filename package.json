{
  "name": "varian-converter",
  "version": "0.3.2",
  "description": "Load and parse varian NMR native format",
  "main": "./lib/index.js",
  "module": "./lib-esm/index.js",
  "types": "./lib/index.d.ts",
  "keywords": [],
  "author": "Luc Patiny",
  "license": "MIT",
  "files": [
    "src",
    "lib",
    "lib-esm"
  ],
  "scripts": {
    "check-types": "tsc --noEmit",
    "clean": "rimraf lib lib-esm",
    "eslint": "eslint src --cache",
    "eslint-fix": "npm run eslint -- --fix",
    "prepack": "npm run tsc",
    "prettier": "prettier --check src",
    "prettier-write": "prettier --write src",
    "test": "npm run test-only && npm run eslint && npm run prettier && npm run check-types",
    "test-only": "vitest run --coverage",
    "wtest": "vitest watch",
    "tsc": "npm run clean && npm run tsc-cjs && npm run tsc-esm",
    "tsc-cjs": "tsc --project tsconfig.cjs.json",
    "tsc-esm": "tsc --project tsconfig.esm.json",
    "demo": "vite --open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheminfo/varian-converter.git"
  },
  "bugs": {
    "url": "https://github.com/cheminfo/varian-converter/issues"
  },
  "homepage": "https://github.com/cheminfo/varian-converter#readme",
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-c8": "^0.30.1",
    "eslint": "^8.36.0",
    "eslint-config-cheminfo-typescript": "^11.3.1",
    "prettier": "^2.8.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-science": "^0.22.0",
    "rimraf": "^4.4.1",
    "typescript": "^5.0.2",
    "vite": "^4.3.3",
    "vitest": "^0.30.1"
  },
  "dependencies": {
    "filelist-utils": "^1.8.1",
    "iobuffer": "^5.3.2",
    "ml-spectra-processing": "^12.0.0"
  }
}
